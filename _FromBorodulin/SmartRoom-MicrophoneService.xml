<!DOCTYPE rdf:RDF [
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
]>

<rdf:RDF xml:base="http://www.cs.karelia.ru/smartroom#"
    xmlns="http://www.cs.karelia.ru/smartroom#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

<!-- Class Microphone Service (subclass of Service) -->

<owl:Class rdf:ID="MicrophoneService">
    <rdfs:subClassOf rdf:resource="#Service"/>
</owl:Class>

<owl:DatatypeProperty rdf:ID="ip">
	<rdfs:domain rdf:resource="#MicrophoneService"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:ID="port">
	<rdfs:domain rdf:resource="#MicrophoneService"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

</rdf:RDF>
