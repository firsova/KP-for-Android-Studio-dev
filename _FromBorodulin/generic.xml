            <owl:Class rdf:about="#ClientNotification"/>
        </owl:unionOf>
    </owl:Class>
    <rdfs:range rdf:resource="#Section"/>
</owl:ObjectProperty>

<!-- Start service mode notification for agenda and presentation services -->
<owl:ObjectProperty rdf:ID="startServiceMode">
    <rdfs:domain>
    <owl:Class>    
